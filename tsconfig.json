{
  "compilerOptions": {
    "target": "es5",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "suppressImplicitAnyIndexErrors": true,
    "lib": [
      "dom",
      "es5",
      "es2015"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": true,
    "outDir": "./dist"
    //"sourceMap": true
  },
  "exclude": [
    "node_modules",
    "lib"
  ],
  "include": [
    "src"
  ]
}
